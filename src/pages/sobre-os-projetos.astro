---
import InsideLayout from "@layouts/InsideLayout.astro"
import H1TitleWithLine from "@components/H1TitleWithLine.astro"
import { Content as Sobre } from "@content/sobre-os-projetos/sobre-os-projetos.mdx"

const canonicalURL = new URL(Astro.url.pathname, Astro.site)
const title = "Sobre os projetos"
---

<InsideLayout
  title={title}
  description="Ofício febril agrega projetos realizados na Universidade e propõe parcerias com artistas. A equipe envolve artistas, professores, escritores, estudantes, designers entre outros colaboradores."
  canonicalURL={canonicalURL}
>
  <H1TitleWithLine title={title} />
  <article>
    <Sobre />
  </article>
</InsideLayout>
