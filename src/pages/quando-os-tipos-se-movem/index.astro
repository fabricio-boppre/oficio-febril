---
import InsideLayout from "@layouts/InsideLayout.astro"
import H1TitleWithLine from "@components/H1TitleWithLine.astro"

const canonicalURL = new URL(Astro.url.pathname, Astro.site)
const title = "Quando os tipos se movem"
---

<InsideLayout
  title={title}
  description="Na sua vocação educativa, o projeto desenvolve oficinas e palestras, tanto abrindo o ateliê para diferentes públicos quanto realizando ações tipográficas fora do espaço da oficina."
  canonicalURL={canonicalURL}
>
  <H1TitleWithLine title={title} />
</InsideLayout>
